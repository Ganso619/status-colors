{
  "key": "com.ganso.status-colors.v3",
  "name": "Custom Status Colors",
  "description": "Override Jira Cloud status lozenge colors site-wide",
  "baseUrl": "https://ganso619.github.io/status-colors",
  "authentication": { "type": "none" },
  "apiVersion": 1,
  "modules": {
    "webResource": [
      {
        "key": "status-colors-resources",
        "name": { "value": "Status Colors CSS" },
        "context": ["atl.general"],
        "resources": {
          "css": [
            { "name": "status-colors", "location": "/css/status-colors.css" }
          ]
        }
      }
    ]
  }
}
